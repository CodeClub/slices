<!DOCTYPE html>
<html>
<head>
  <title><%= @page.name %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body class="layout_two">
  <header>
    <nav>
      <%= primary_navigation %>
      <%= secondary_navigation %>
    </nav>
  </header>

  <%= wrap_article_in_article_tag(@page) do %>
    <%= container "container_one" %>
  <% end %>
  </article>
  <aside class="container_two">
    <%= container "container_two" %>
  </aside>
  <footer>
    <p><%= t('address') %></p>
    <p><%= t('address.html') %></p>
  </footer>
</body>
</html>
